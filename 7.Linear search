#include<stdio.h>
int main()
{
int n,i,key,a[10],flag=0;
printf("enter n:");
scanf("%d",&n);
	for(i=0;i<n;i++)
  scanf("%d",&a[i]);
  printf("enter key:");
  scanf("%d",&key);
  for(i=0;i<n;i++)
  {
  if(a[i]==key)
  flag=1;
  break;
}
if(flag==1)
  printf("element is present");
  else
  printf("element is not present");
  
  return 0;
  }
